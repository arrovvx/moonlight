extends layout

block style
	link(rel="stylesheet", href="/stylesheets/navigation.css")
	link(rel="stylesheet", href="/stylesheets/index.css")

block header
	include ./navigation.jade
	
block content
	.index
		ul(class="navList")
			each name, link in navListObj
				if !(link=="index")
					li(class="navListItem")
						a(href="/#{link}") #{name}
